<body>
  <h1>FORM</h1>

  <form method="POST" action="/submit" enctype="multipart/form-data">
    <h2>NPM: </h2>
    <input type="text" placeholder="npm" name="npm">

    <h2>Nama: </h2>
    <input type="text" placeholder="name" name="name">

    <h2>Email: </h2>
    <input type="text" placeholder="email" name="email">

